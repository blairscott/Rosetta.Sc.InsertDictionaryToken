<?xml version="1.0" encoding="utf-8" ?>
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <RichText.InsertDictionaryToken>
    <FormDialog Icon="Network/32x32/link.png" Header="Insert a dictionary token" Text="Select the dictionary item to use" OKButton="Select">
      <script type="text/javascript" src="/sitecore/shell/Controls/Rich Text Editor/InsertDictionaryToken/InsertDictionaryToken.js">.</script>
      <CodeBeside Type="Rosetta.Sc.InsertDictionaryToken.Controls.InsertDictionaryToken, Rosetta.Sc.InsertDictionaryToken" />

      <DataContext ID="InternalLinkDataContext"/>
